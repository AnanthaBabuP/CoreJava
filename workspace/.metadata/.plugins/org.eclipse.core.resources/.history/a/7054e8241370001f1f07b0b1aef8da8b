package com.MultiThreading.MainMultithreading;

public class MainMultiThreadDemo {

	public static void main(String[] args) {
		
		try {
			
			Thread t;
			Runnable obj1 = new MainThreadDemo("Thread-1"); // Object Creation
			Thread t1 = new Thread(obj1);
			t1.start();
			
			Runnable obj2 = new MainThreadDemo("Thread-2"); // Object creation
			Thread t2 = new Thread(obj2);
			t2.start();
			
			Runnable obj3 = new MainThreadDemo("Thread-3"); // Object Creation
			Thread t3 = new Thread(obj3);
			t3.start();
			
			System.out.println(t.isAlive());
			System.out.println("Thread "+Thread.currentThread().getName() + " Start");
			for(int i=1;i<=5;i++) {
				System.out.println(Thread.currentThread().getName() +i);
				Thread.sleep(1000);
			}
			System.out.println("Thread "+ Thread.currentThread().getName()+" End");
			
		} catch(Exception e) {
			System.out.println(e.getMessage());
			
		}
	}

}
