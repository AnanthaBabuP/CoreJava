package com.FunctionalInterface;

public class Book implements Comparable{

	String name;
	int page;
	double price;
	
	
	public Book(String name, int page, double price) {
		super();
		this.name = name;
		this.page = page;
		this.price = price;
	}


	@Override
	public int compareTo(Object x) {
		Book b1 = (Book)x;
		if(page == b1.page)
			return 1;
		else if(page > b1.page)
			return 0;
		else 
			return -1;
		
	}
	
	
}
